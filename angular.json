{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "food-manager": {
            "root": "src",
            "sourceRoot": "src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@angular-builders/custom-webpack:browser",
                    "options": {
                        "optimization": false,
                        "outputHashing": "all",
                        "sourceMap": true,
                        "namedChunks": false,
                        "aot": false,
                        "extractLicenses": true,
                        "vendorChunk": false,
                        "buildOptimizer": false,
                        "outputPath": "dist",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "polyfills": "src/polyfills.ts",
                        "assets": [
                            "src/assets",
                            "src/assets/icons/favicon.ico",
                            "src/assets/icons/favicon.png",
                            "src/assets/icons/favicon.icns",
                            "src/assets/icons/favicon.256x256.png",
                            "src/assets/icons/favicon.512x512.png",
                            "src/assets/svg/*"
                        ],
                        "styles": [
                            "src/styles.scss"
                        ],
                        "customWebpackConfig": {
                            "path": "./angular.webpack.web.js"
                        },
                        "allowedCommonJsDependencies": [
                            "lodash",
                            "moment",
                            "file-saver",
                            "pako",
                            "currency.js"
                        ]
                    },
                    "configurations": {
                        "web-mock": {
                            "baseHref": "/",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.mock.ts"
                                }
                            ],
                            "customWebpackConfig": {
                                "path": "./angular.webpack.web.js"
                            }
                        },
                        "web-local": {
                            "baseHref": "/",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.local.ts"
                                }
                            ],
                            "customWebpackConfig": {
                                "path": "./angular.webpack.web.js"
                            }
                        },
                        "web-dev": {
                            "baseHref": "/",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.dev.ts"
                                }
                            ],
                            "customWebpackConfig": {
                                "path": "./angular.webpack.web.js"
                            }
                        },
                        "web-test": {
                            "baseHref": "/",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.test.ts"
                                }
                            ],
                            "customWebpackConfig": {
                                "path": "./angular.webpack.web.js"
                            }
                        },
                        "web-staging": {
                            "baseHref": "/",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.staging.ts"
                                }
                            ],
                            "customWebpackConfig": {
                                "path": "./angular.webpack.web.js"
                            }
                        },
                        "web-production": {
                            "baseHref": "/",
                            "optimization": true,
                            "aot": true,
                            "buildOptimizer": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "customWebpackConfig": {
                                "path": "./angular.webpack.web.js"
                            }
                        },
                        "el-mock": {
                            "aot": true,
                            "buildOptimizer": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.mock.ts"
                                }
                            ],
                            "customWebpackConfig": {
                                "path": "./angular.webpack.el.js"
                            }
                        },
                        "el-local": {
                            "aot": true,
                            "buildOptimizer": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.local.ts"
                                }
                            ],
                            "customWebpackConfig": {
                                "path": "./angular.webpack.el.js"
                            }
                        },
                        "el-dev": {
                            "aot": true,
                            "buildOptimizer": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.dev.ts"
                                }
                            ],
                            "customWebpackConfig": {
                                "path": "./angular.webpack.el.js"
                            }
                        },
                        "el-test": {
                            "aot": true,
                            "buildOptimizer": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.test.ts"
                                }
                            ],
                            "customWebpackConfig": {
                                "path": "./angular.webpack.el.js"
                            }
                        },
                        "el-staging": {
                            "optimization": true,
                            "aot": true,
                            "buildOptimizer": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.staging.ts"
                                }
                            ],
                            "customWebpackConfig": {
                                "path": "./angular.webpack.el.js"
                            }
                        },
                        "el-production": {
                            "optimization": true,
                            "aot": true,
                            "buildOptimizer": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "customWebpackConfig": {
                                "path": "./angular.webpack.el.js"
                            }
                        },
                        "el-buildcheck-dev": {
                            "optimization": true,
                            "aot": true,
                            "buildOptimizer": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.dev.ts"
                                }
                            ],
                            "customWebpackConfig": {
                                "path": "./angular.webpack.el.js"
                            }
                        },
                        "el-buildcheck-mock": {
                            "optimization": true,
                            "aot": true,
                            "buildOptimizer": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.mock.ts"
                                }
                            ],
                            "customWebpackConfig": {
                                "path": "./angular.webpack.el.js"
                            }
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-builders/custom-webpack:dev-server",
                    "options": {
                        "browserTarget": "food-manager:build"
                    },
                    "configurations": {
                        "web-mock": {
                            "browserTarget": "food-manager:build:web-mock"
                        },
                        "web-local": {
                            "browserTarget": "food-manager:build:web-local"
                        },
                        "web-dev": {
                            "browserTarget": "food-manager:build:web-dev"
                        },
                        "web-test": {
                            "browserTarget": "food-manager:build:web-test"
                        },
                        "web-staging": {
                            "browserTarget": "food-manager:build:web-staging"
                        },
                        "web-production": {
                            "browserTarget": "food-manager:build:web-production"
                        },
                        "el-local": {
                            "browserTarget": "food-manager:build:el-local"
                        },
                        "el-mock": {
                            "browserTarget": "food-manager:build:el-mock"
                        },
                        "el-dev": {
                            "browserTarget": "food-manager:build:el-dev"
                        },
                        "el-staging": {
                            "browserTarget": "food-manager:build:el-staging"
                        },
                        "el-production": {
                            "browserTarget": "food-manager:build:el-production"
                        },
                        "el-buildcheck-dev": {
                            "browserTarget": "food-manager:build:el-buildcheck-dev"
                        },
                        "el-buildcheck-mock": {
                            "browserTarget": "food-manager:build:el-buildcheck-mock"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                      "buildTarget": "food-manager:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills-test.ts",
                        "codeCoverage": true,
                        "tsConfig": "src/tsconfig.spec.json",
                        "karmaConfig": "src/karma.conf.js",
                        "styles": [
                            "src/styles.scss"
                        ],
                        "assets": [
                            "src/assets",
                            "src/assets/icons/favicon.ico",
                            "src/assets/icons/favicon.png",
                            "src/assets/icons/favicon.icns",
                            "src/assets/icons/favicon.256x256.png",
                            "src/assets/icons/favicon.512x512.png"
                        ]
                    }
                }
            }
        },
        "food-manager-e2e": {
            "root": "e2e",
            "projectType": "application"
        }
    },
    "schematics": {
        "@schematics/angular:component": {
            "prefix": "app",
            "style": "scss",
            "changeDetection": "OnPush",
            "skipTests": true
        },
        "@schematics/angular:directive": {
            "prefix": "app",
            "skipTests": true
        },
        "@schematics/angular:service": {
            "skipTests": true
        },
        "@schematics/angular:class": {
            "skipTests": true
        },
        "@schematics/angular:pipe": {
            "skipTests": true
        }
    },
    "cli": {
      "analytics": false
    }
}
